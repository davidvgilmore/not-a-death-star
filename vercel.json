{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/main.js", "dest": "/main.js" },
    { "src": "/social-share.png", "dest": "/public/social-share.png" },
    { "handle": "filesystem" }
  ]
}
